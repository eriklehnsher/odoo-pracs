<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="stock_document_type_view_tree" model="ir.ui.view">
            <field name="name">stock.document.type.view.tree</field>
            <field name="model">stock.document.type</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="code"/>
                    <field name="name"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>

        <record id="stock_document_type_view_action" model="ir.actions.act_window">
            <field name="name">Stock Document Type</field>
            <field name="res_model">stock.document.type</field>
            <field name="view_mode">tree</field>
        </record>

        <record id="stock_customs_declaration_view_tree" model="ir.ui.view">
            <field name="name">stock.customs.declaration.view.tree</field>
            <field name="model">stock.customs.declaration</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="number_declaration"/>
                    <field name="name"/>
                    <field name="product_brand_ids" widget="many2many_tags"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>

        <record id="stock_customs_declaration_view_action" model="ir.actions.act_window">
            <field name="name">Stock Customs Declaration</field>
            <field name="res_model">stock.customs.declaration</field>
            <field name="view_mode">tree</field>
        </record>

        <record id="stock_accounting_type_view_tree" model="ir.ui.view">
            <field name="name">stock.accounting.type.view.tree</field>
            <field name="model">stock.accounting.type</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="code"/>
                    <field name="name"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>

        <record id="stock_accounting_type_view_action" model="ir.actions.act_window">
            <field name="name">Stock Accounting Type</field>
            <field name="res_model">stock.accounting.type</field>
            <field name="view_mode">tree</field>
        </record>

    </data>
</odoo>