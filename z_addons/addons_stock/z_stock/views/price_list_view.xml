<?xml version="1.0" encoding="UTF-8" ?>
<odoo>
    <data>
        <record id="price_list_view_tree" model="ir.ui.view">
            <field name="name">price.list.view.tree</field>
            <field name="model">price.list</field>
            <field name="arch" type="xml">
                <tree editable="bottom">
                    <field name="name"/>
<!--                    <field name="currency_id"/>-->
                    <field name="rounding_factor"/>
                    <field name="description"/>
                </tree>
            </field>
        </record>

        <record id="price_list_view_action" model="ir.actions.act_window">
            <field name="name">Price Lists</field>
            <field name="res_model">price.list</field>
            <field name="view_mode">tree</field>
        </record>
    </data>
</odoo>